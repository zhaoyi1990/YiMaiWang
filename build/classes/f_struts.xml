<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" 
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="f_struts" namespace="/" extends="struts-default">
		<action name="loginAction" class="com.ymw.action.f.LoginAction">
			<result name="manage" type="redirect">manage/index.jsp</result>
			<result name="success">index.jsp</result>
			<result name="error">login.jsp</result>
		</action>
		
		<action name="reg_*" class="com.ymw.action.f.RegAction" method="{1}">
			<result name="success">reg-result.html</result>
			<result name="error">register.jsp</result>
		</action>

		<action name="*">
			<result>{1}.jsp</result>
		</action>
	</package>
</struts>    
